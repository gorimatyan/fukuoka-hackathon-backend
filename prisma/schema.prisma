generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model FloodHazardZones {
  id          Int                     @id @default(autoincrement())
  hazard_type Int
  a31_101     String?
  a31_102     String?
  a31_103     Int?
  a31_104     String?
  a31_105     Int?
  a31_106     String?
  a31_107     String?
  a31_108     String?
  a31_201     String?
  a31_202     String?
  a31_203     Int?
  a31_204     String?
  a31_205     Int?
  a31_206     String?
  a31_207     String?
  a31_301     String?
  a31_302     String?
  a31_303     Int?
  a31_304     String?
  a31_305     Int?
  a31_306     String?
  a31_307     String?
  a31_308     String?
  a31_401     String?
  a31_402     String?
  a31_403     Int?
  a31_404     String?
  a31_405     Int?
  a31_406     String?
  a31_407     String?
  a31_408     String?
  geometry    Unsupported("geometry")

  @@map("flood_hazard_zones")
}

model LandslideZones {
  id       Int                      @id @default(autoincrement())
  a33_001  String?                  @db.VarChar
  a33_002  String?                  @db.VarChar
  a33_003  String?                  @db.VarChar
  a33_004  String?                  @db.VarChar
  a33_005  String?                  @db.VarChar
  a33_006  String?                  @db.VarChar
  a33_007  String?                  @db.VarChar
  a33_008  String?                  @db.VarChar
  geometry Unsupported("geometry")?

  @@index([geometry], map: "landslide_zones_geometry_geom_idx", type: Gist)
  @@map("landslide_zones")
}

/// This table contains check constraints and requires additional setup for migrations. Visit https://pris.ly/d/check-constraints for more info.
model SpatialRefSys {
  srid      Int     @id
  auth_name String? @db.VarChar(256)
  auth_srid Int?
  srtext    String? @db.VarChar(2048)
  proj4text String? @db.VarChar(2048)

  @@map("spatial_ref_sys")
}

model TsunamiHazardZones {
  gid     Int                      @id @default(autoincrement())
  a40_001 String?                  @db.VarChar(10)
  a40_002 String?                  @db.VarChar(2)
  a40_003 String?                  @db.VarChar(40)
  geom    Unsupported("geometry")?

  @@index([geom], type: Gist)
  @@map("tsunami_hazard_zones")
}
